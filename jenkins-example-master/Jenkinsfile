pipeline {
agent any

stages {
    stage('Checkout') {
        steps {
            checkout([$class: 'GitSCM',
                      branches: [[name: '*/main']],
                      userRemoteConfigs: [[url: 'https://github.com/Marvin1808/jenkinsPipeline.git']]])
        }
    }
    
    stage('Build') {
        steps {
            sh 'mvn clean package'
        }
    }
    
    stage('Test') {
        steps {
            sh 'mvn test'
        }
    }
  }
}
