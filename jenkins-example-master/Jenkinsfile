pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'No build necessary'
            }
        }
        stage('Test') {
            steps {
                sh 'javac -cp junit-platform-console-standalone-1.7.0.jar:. jenkins-example-master/src/test/java/com/techprimers/testing/FizzBuzzTest.java'
                sh 'java -jar junit-platform-console-standalone-1.7.0.jar --class-path . --scan-classpath --details=tree'
            }
        }
    }
}
