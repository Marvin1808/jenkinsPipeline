pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh 'echo "No build necessary"'
            }
        }
        stage('Test') {
            steps {
                sh 'javac -cp junit-platform-console-standalone-1.7.0.jar:. src/test/java/com/techprimers/testing/FizzBuzzTest.java'
                sh 'java -jar junit-platform-console-standalone-1.7.0.jar -cp . --select-class src/test/java/com/techprimers/testing/FizzBuzzTest.java'
            }
        }
    }
}
